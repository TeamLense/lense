{
    "manifest_version": 2,
  
    "name": "18-",
    "description": "This extension blocks NSFW images",
    "version": "1.0",
  
    "browser_action": {
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        },
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "storage"
    ]
}