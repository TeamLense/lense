{
    "manifest_version": 2,
  
    "name": "18-",
    "description": "This extension blocks NSFW images",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "html/popup.html"
    },

    "permissions": [
      "activeTab",
      "storage"
    ],

    "background": {
      "scripts": ["js/jquery-3.2.1.min.js", "js/popup.js"]
    }
}